{
  "name": "jalgoarena",
  "version": "1.0.48",
  "description": "JAlgoArena UI",
  "main": "server.js",
  "scripts": {
    "installRequiredGlobalDependencies": "npm install webpack -g & npm install pm2 -g",
    "test": "jest && codecov",
    "pretest": "./node_modules/.bin/flow check",
    "test:watch": "jest --watch",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node server.js",
    "prepm2start": "npm run build",
    "pm2start": "cross-env NODE_ENV=production pm2 start server.js --name jalgoarena-ui",
    "postinstall": "npm run herokuBuild",
    "herokuStart": "cross-env NODE_ENV=production node server.js",
    "dev": "node server.js --trace-sync-io",
    "prod": "cross-env NODE_ENV=production node server.js",
    "prebuild": "node deletePublicDir.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "herokuBuild": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --config webpack.config.prod.js",
    "predeploy": "npm run build",
    "deploy": "node createReleasePackage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spolnik/JAlgoArena-UI.git"
  },
  "keywords": [
    "JAlgoArena"
  ],
  "author": "Jacek Sp√≥lnik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spolnik/JAlgoArena-UI/issues"
  },
  "homepage": "https://github.com/spolnik/JAlgoArena-UI#readme",
  "dependencies": {
    "archiver": "^1.3.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "brace": "^0.9.1",
    "compression": "^1.6.2",
    "cp-file": "^4.1.1",
    "del": "^2.2.2",
    "express": "^4.14.1",
    "helmet": "^3.4.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "react": "^15.4.2",
    "react-ace": "^4.1.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-bootstrap-table": "^2.11.1",
    "react-dom": "^15.4.2",
    "react-json-tree": "^0.10.1",
    "react-mixins": "^0.1.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "react-spin": "^0.6.2",
    "redbox-react": "^1.3.3",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-filter-actions": "^1.2.0",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.7.1",
    "s": "^0.1.1",
    "serve-static": "^1.11.2",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cross-env": "^5.0.1",
    "flow-bin": "^0.38.0",
    "jest": "^18.1.0",
    "nock": "^9.0.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
